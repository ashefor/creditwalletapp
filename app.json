{
  "name": "Credit Wallet",
  "displayName": " Credit Wallet Mobile App ",
  "expo": {
    "scheme": "creditwallet",
    "name": "Credit Wallet",
    "splash": {
      "image": "src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "icon": "src/assets/images/logo.png",
    "slug": "credit-wallet",
    "privacy": "public",
    "orientation": "portrait",
    "version": "1.0.0",
    "ios": {
      "bundleIdentifier": "com.princepsfinance.creditwallet",
      "buildNumber": "1.0.0",
      "usesIcloudStorage": true
    },
    "updates": {
      "fallbackToCacheTimeout": 30000
    },
    "android": {
      "package": "com.princepsfinance.creditwallet",
      "enableDangerousExperimentalLeanBuilds": true,
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "http",
              "host": "test.compound.ng"
            },
            {
              "scheme": "https",
              "host": "accounts.creditwallet.ng"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "MANAGE_DOCUMENTS",
        "READ_INTERNAL_STORAGE",
        "USE_FINGERPRINT"
      ]
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "description": "Take Loans within 30 minutes and no collateral!"
  }
}
